{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "healthcare-project21-df"
		},
		"DataLakeStorage_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStorage_ls'"
		},
		"DatabricksDeltaLake_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'DatabricksDeltaLake_ls'"
		},
		"hospital_sql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hospital_sql_ls'"
		},
		"DataLakeStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healthcareadlsgen.dfs.core.windows.net/"
		},
		"hospital_sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "healthcareserver.database.windows.net"
		},
		"hospital_sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hospital_sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Dreamy"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "healthcare-project21-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeStorage_ls_accountKey": {
						"value": "[parameters('DataLakeStorage_ls_accountKey')]"
					},
					"DatabricksDeltaLake_ls_accessToken": {
						"value": "[parameters('DatabricksDeltaLake_ls_accessToken')]"
					},
					"hospital_sql_ls_password": {
						"value": "[parameters('hospital_sql_ls_password')]"
					},
					"DataLakeStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStorage_ls_properties_typeProperties_url')]"
					},
					"hospital_sql_ls_properties_typeProperties_server": {
						"value": "[parameters('hospital_sql_ls_properties_typeProperties_server')]"
					},
					"hospital_sql_ls_properties_typeProperties_database": {
						"value": "[parameters('hospital_sql_ls_properties_typeProperties_database')]"
					},
					"hospital_sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('hospital_sql_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}